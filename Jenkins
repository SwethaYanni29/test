pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'This is build application'
            }
        }
        stage('Test') {
            steps {
                echo 'This is test1'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is deploy application'
            }
        }
    }
  post
  {
    always
    {
      emailext body: 'summary', replyTo: 'swethayprahallad@gmail.com', subject: 'email', to: 'swethayprahallad@gmail.com'
    }
  }
}
